{
  "name": "person-ui",
  "version": "0.4.0",
  "description": "person user interface",
  "main": "index.js",
  "scripts": {
    "serve-profiles": "beefy -p 5000 examples/profiles/app.js:bundle.js --index examples/profiles/index.html",
    "serve-graph": "beefy -p 5000 examples/graph/app.js:bundle.js --index examples/graph/index.html",
    "config": "node examples/profiles/config.js & node examples/graph/config.js",
    "profiles": "npm run config && npm run serve-profiles",
    "graph": "npm run config && npm run serve-graph",
    "test": "node test/index.js"
  },
  "browserify": {
    "transform": [
      "cssify",
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openappjs/person-ui.git"
  },
  "author": "openappjs",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/openappjs/person-ui/issues"
  },
  "homepage": "https://github.com/openappjs/person-ui",
  "dependencies": {
    "aviator": "^0.4.3",
    "dagre": "^0.6.4",
    "debug": "^1.0.4",
    "domready": "^1.0.7",
    "extend": "^1.3.0",
    "font-awesome": "^4.2.0",
    "geval": "^2.1.1",
    "is-array": "^1.0.1",
    "key-swap": "0.0.2",
    "lodash": "^2.4.1",
    "mercury": "^6.0.1",
    "reduce": "^1.0.1",
    "bluebird": "~2.3.11",
    "highland": "~2.0.0",
    "node-stringify": "0.0.18"
  },
  "devDependencies": {
    "atomify-css": "^3.2.0",
    "beefy": "^2.1.0",
    "brfs": "^1.2.0",
    "cssify": "^0.5.1",
    "debug": "^1.0.4",
    "envify": "^2.0.1",
    "global": "^4.2.1",
    "isarray": "0.0.1",
    "mercury-fa": "git://github.com/openappjs/mercury-fa",
    "raf": "^2.0.3",
    "tape": "^2.14.0"
  }
}
